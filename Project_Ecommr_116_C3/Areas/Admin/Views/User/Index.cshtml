<div class="row">
    <div class="col-9">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-3">
        <a asp-page="/Account/Register" asp-area="Identity" class="btn btn-info form-control">
            <i class="fas fa-plus"></i>&nbsp;New User
        </a>
    </div>
</div>
<div class="container rounded border">
    <table id="tblData" class="table table-hover table-bordered table-active table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts{
    <script src="~/js/User.js"></script>
}